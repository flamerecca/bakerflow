{
    "name": "flamerecca/bakerflow",
    "description": "Neatly Make BREAD in Laravel",
    "keywords": ["laravel", "api", "bakerflow"],
    "license": "MIT",
    "homepage": "https://gitpage.reccachao.net/bakerflow/",
    "support": {
        "issues": "https://github.com/flamerecca/bakerflow/issues",
        "source": "https://github.com/flamerecca/bakerflow"
    },
    "authors": [
        {
            "name": "Recca Chao",
            "email": "flamerecca711@gmail.com"
        }
    ],
    "require": {
        "tcg/voyager": "~1.1",
        "prettus/l5-repository": "~2.6"
    },
    "require-dev": {
        "phpunit/phpcov": "~3.0|~4.0",
        "phpunit/phpunit": "~5.7.14|~6.1|~7.0",
        "laravel/framework": "~5.4.36|~5.5.0|~5.6.0",
        "orchestra/testbench": "~3.4.11|~3.5.0|~3.6.0",
        "orchestra/database": "~3.4.0|~3.5.0|~3.6.0",
        "orchestra/testbench-core": "~3.4.5|~3.5.0|~3.6.0",
        "laravel/browser-kit-testing": "~1.0.0|~2.0.0",
        "orchestra/testbench-browser-kit": "~3.4.1|~3.5.0|~3.6.0"
    },
    "autoload": {
        "psr-4": {
            "Flamerecca\\Bakerflow\\": "src/Prettus/Repository/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Flamerecca\\Bakerflow\\Tests\\": "tests/"
      }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Flamerecca\\Bakerflow\\BakerflowServiceProvider",
                "Flamerecca\\Bakerflow\\Providers\\BakerflowDummyBreadServiceProvider"
            ]
        }
    }
}
